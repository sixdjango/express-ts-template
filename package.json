{
  "name": "chatgpt-web-service",
  "version": "1.0.0",
  "private": false,
  "description": "ChatGPT Web Service",
  "author": "ChenZhaoYu <chenzhaoyu1994@gmail.com>",
  "keywords": [
    "chatgpt-web",
    "chatgpt",
    "chatbot",
    "express"
  ],
  "engines": {
    "node": "^16 || ^18"
  },
  "scripts": {
    "pm2": "pm2 startOrReload pm2.config.js --env production",
    "prisma": "prisma generate && prisma migrate dev --name init --preview-feature",
    "start": "esno ./src/index.ts",
    "dev": "esno watch ./src/index.ts",
    "prod": "esno ./build/index.js",
    "build": "npm run clean && tsup",
    "clean": "rimraf build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "common:cleanup": "rimraf node_modules && rimraf package-lock.json"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^3.2.0",
    "esno": "^0.16.3",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "isomorphic-fetch": "^3.0.0",
    "node-fetch": "^3.3.0",
    "prisma": "^4.13.0",
    "socket.io": "^4.6.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.35.3",
    "@types/express": "^4.17.17",
    "@types/node": "^18.14.6",
    "eslint": "^8.35.0",
    "rimraf": "^4.2.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  }
}
